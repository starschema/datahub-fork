# DataHub Production Environment Variables Template
# Copy this file to .env and update with your production values
# IMPORTANT: Never commit .env file to git - it contains secrets!

# ===== IMAGE VERSIONS =====
DATAHUB_VERSION=v1.3.0.1
DATAHUB_FRONTEND_IMAGE=ghcr.io/starschema/custom-datahub-frontend-react:latest
DATAHUB_ACTIONS_IMAGE=ghcr.io/starschema/datahub-actions:latest
DATAHUB_GMS_IMAGE=acryldata/datahub-gms

# ===== SECRETS - CHANGE THESE IN PRODUCTION! =====
# DataHub secret key - used for session encryption
DATAHUB_SECRET=CHANGE_ME_TO_STRONG_RANDOM_STRING

# MySQL credentials
MYSQL_ROOT_PASSWORD=CHANGE_ME_TO_STRONG_PASSWORD
MYSQL_PASSWORD=CHANGE_ME_TO_STRONG_PASSWORD

# Neo4j credentials (if using external Neo4j)
NEO4J_PASSWORD=CHANGE_ME_TO_STRONG_PASSWORD

# DataHub system client credentials
DATAHUB_SYSTEM_CLIENT_SECRET=CHANGE_ME_TO_STRONG_RANDOM_STRING

# ===== AI ASSISTANT CONFIGURATION =====
# LLM Provider (gemini, openai, anthropic, etc.)
LLM_PROVIDER=gemini

# Gemini API configuration
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE
GEMINI_MODEL=gemini-2.0-flash-exp

# ===== OPTIONAL: PORT OVERRIDES =====
# Uncomment and modify if you need different ports
# DATAHUB_MAPPED_GMS_PORT=8888
# DATAHUB_MAPPED_ELASTIC_PORT=9200
# DATAHUB_MAPPED_MYSQL_PORT=3306

# ===== OPTIONAL: FEATURE FLAGS =====
METADATA_SERVICE_AUTH_ENABLED=true
DATAHUB_TELEMETRY_ENABLED=true
